{
  "useWorkspaces": true,
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "version": "0.0.8",
  "command": {
    "publish": {
      "registry": "http://r.npm.guorou.net"
    }
  },
  "versionRef": "tag",
  "preid": "beta",
  "verifyAccess": false,
  "verifyRegistry": false,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.test.*",
    "**/*.md"
  ]
}
